<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.ChatbotMapper">

       <!-- 채팅 내용 저장 -->
 <insert id="insertMessage" parameterType="com.example.demo.model.Chatbot">
    INSERT INTO chatbot (client_id, sender, message, reg_date)
    VALUES (#{clientId}, #{sender}, #{message}, #{regDate})
</insert>
    
</mapper>

